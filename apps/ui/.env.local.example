# Next.js local development file (.env.local)
# This file is loaded automatically by Next.js during dev
# Create .env.local in this directory and copy values below

# Local Supabase (for development with `pnpm supabase start`)
# Uncomment these when running local Supabase - get from `pnpm supabase status`
# NEXT_PUBLIC_SUPABASE_URL=http://127.0.0.1:54321
# NEXT_PUBLIC_SUPABASE_ANON_KEY=your_local_anon_key
# SUPABASE_SERVICE_ROLE_KEY=your_local_service_role_key

# Cloud Supabase (default for production)
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Encryption (must match worker)
ENCRYPTION_KEY=your_encryption_key_256_bit_hex
